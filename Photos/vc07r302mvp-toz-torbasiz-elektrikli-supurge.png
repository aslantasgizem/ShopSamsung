    <!-- for DIL and other DMP related tags -->
    <script>
        let dmp_ready_interval_id;
        let dmp_ready_try_count = 0;
        // Urun detay sayfalari icin DMP_Ready event DMP.ProductPrice gelene kadar (0 dan buyuk olana kadar) ertelendi.
        // Galaxy Note9 gibi, verisini servisten alan sayfalar icin gecerli olacak.
        if (document.documentElement.classList.contains('html-product-details-page')) {
            dmp_ready_interval_id = setInterval(function () {
                if (parseInt(DMP.ProductPrice) > 0) {
                    window.dataLayer.push({ 'event': 'DMP_Ready' });
                    clearInterval(dmp_ready_interval_id);
                    document.body.dataset.status = "ready";
                }
                dmp_ready_try_count++;
                if (dmp_ready_try_count > 50) {
                    window.dataLayer.push({ 'event': 'DMP_Ready' });
                    clearInterval(dmp_ready_interval_id);
                }
            }, 200);
        } else {
            window.dataLayer.push({ 'event': 'DMP_Ready' });
        }
    </script>
